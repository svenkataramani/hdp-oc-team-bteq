.SET ECHOREQ ON
.SET ERROROUT STDOUT
.SET WIDTH 256


.LOGON godaddy3.prod.phx3.gdg/P_BTCH_HadoopImp,P_BTCH_HadoopImp;

SELECT TOP 1 * FROM P_HadoopImp_S.ShopDomainRegNaics_STG;

.IF ACTIVITYCOUNT = 0 THEN .GOTO E086

delete P_ARM_IMPORT.ShopDomainRegNaics all;

.IF ERRORCODE <> 0 THEN .GOTO E003

insert into P_ARM_IMPORT.ShopDomainRegNaics
