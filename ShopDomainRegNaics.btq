FROM P_HadoopImp_S.ShopDomainRegNaics_STG;

.IF ERRORCODE <> 0 THEN .GOTO E003

COLLECT STATS P_ARM_IMPORT.ShopDomainRegNaics COLUMN(shopper_id);

.IF ERRORCODE <> 0 THEN .GOTO E003

DELETE P_HADOOPIMP_S.ShopDomainRegNaics_STG ALL;

.IF ERRORCODE <> 0 THEN .GOTO E003

